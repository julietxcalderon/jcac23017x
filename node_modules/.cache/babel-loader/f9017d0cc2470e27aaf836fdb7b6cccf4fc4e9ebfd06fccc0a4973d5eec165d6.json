o;\r\n    this.descripcion = descripcion;\r\n  }\r\n}\r\n\r\nclass Inventario {\r\n  constructor() {\r\n    this.leyes = [];\r\n  }\r\n\r\n  agregar_ley(ley) {\r\n    this.leyes.push(ley);\r\n  }\r\n\r\n  obtener_leyes() {\r\n    return this.leyes;\r\n  }\r\n\r\n  actualizar_ley(codigo, nueva_descripcion) {\r\n    for (let ley of this.leyes) {\r\n      if (ley.codigo === codigo) {\r\n        ley.descripcion = nueva_descripcion;\r\n        break;\r\n      }\r\n    }\r\n  }\r\n\r\n  eliminar_ley(codigo) {\r\n    for (let ley of this.leyes) {\r\n      if (ley.codigo === codigo) {\r\n        this.leyes.splice(this.leyes.indexOf(ley), 1);\r\n        break;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\nclass Carrito {\r\n  constructor() {\r\n    this.leyes = [];\r\n  }\r\n\r\n  agregar_ley(ley) {\r\n    this.leyes.push(ley);\r\n  }\r\n\r\n  obtener_leyes() {\r\n    return this.leyes;\r\n  }\r\n\r\n  actualizar_ley(codigo, nueva_descripcion) {\r\n    for (let ley of this.leyes) {\r\n      if (ley.codigo === codigo) {\r\n        ley.descripcion = nueva_descripcion;\r\n        break;\r\n      }\r\n    }\r\n  }\r\n\r\n  eliminar_ley(codigo) {\r\n    for (let ley of this.leyes) {\r\n      if (ley.codigo === codigo) {\r\n        this.leyes.splice(this.leyes.indexOf(ley), 1);\r\n        break;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\nexport default {\r\n  name: 'FormularioLeyes',\r\n  data() {\r\n    return {\r\n      ley: {\r\n        codigo: '',\r\n        descripcion: '',\r\n      },\r\n      leyes: [],\r\n      inventario: new Inventario(),\r\n      carrito: new Carrito(),\r\n    };\r\n  },\r\n  methods: {\r\n    agregarLey() {\r\n      const nuevaLey = new Ley(this.ley.codigo, this.ley.descripcion);\r\n      this.inventario.agregar_ley(nuevaLey);\r\n\r\n      // Enviar la ley a la base de datos\r\n      axios\r\n        .post('https://julietxcalderon.pythonanywhere.com/leyes', nuevaLey)\r\n        .then(() => {\r\n          console.log('Ley guardada correctamente');\r\n          this.resetForm();\r\n        })\r\n        .catch(error => {\r\n          console.error('Error al guardar la ley:', error);\r\n        });\r\n    },\r\n    resetForm() {\r\n      this.ley.codigo = '';\r\n      this.ley.descripcion = '';\r\n    },\r\n    leerLeyes() {\r\n      // Obtener las leyes de la base de datos\r\n      axios\r\n        .get('https://julietxcalderon.pythonanywhere.com/leyes')\r\n        .then(response => {\r\n          this.leyes = response.data;\r\n          console.log('Leyes obtenidas correctamente');\r\n        })\r\n        .catch(error => {\r\n          console.error('Error al obtener las leyes:', error);\r\n        });\r\n    },\r\n    actualizarLey() {\r\n      const { codigo, descripcion } = this.ley;\r\n      this.inventario.actualizar_ley(codigo, descripcion);\r\n\r\n      // Actualizar la ley en la base de datos\r\n      axios\r\n        .put(`https://julietxcalderon.pythonanywhere.com/leyes/${codigo}`, {\r\n          descripcion: descripcion,\r\n        })\r\n        .then(() => {\r\n          console.log('Ley actualizada correctamente');\r\n          this.leerLeyes();\r\n          this.resetForm();\r\n        })\r\n        .catch(error => {\r\n          console.error('Error al actualizar la ley:', error);\r\n        });\r\n    },\r\n    eliminarLey() {\r\n      const { codigo } = this.ley;\r\n      this.inventario.eliminar_ley(codigo);\r\n\r\n      // Eliminar la ley de la base de datos\r\n      axios\r\n        .delete(`https://julietxcalderon.pythonanywhere.com/leyes/${codigo}`)\r\n        .then(() => {\r\n          console.log('Ley eliminada correctamente');\r\n          this.leerLeyes();\r\n          this.resetForm();\r\n        })\r\n        .catch(error => {\r\n          console.error('Error al eliminar la ley:', error);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    this.leerLeyes();\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.formulario-leyes {\r\n  background-color: #000000;\r\n  padding: 20px;\r\n  border-rad
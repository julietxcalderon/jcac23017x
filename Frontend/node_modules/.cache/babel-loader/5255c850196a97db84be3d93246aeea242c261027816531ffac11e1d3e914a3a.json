{"ast":null,"code":"import NavBar from './components/NavBar.vue';\nimport LegislacionArg from './components/LegislacionArg.vue';\nimport JurisprudenciaArg from './components/JurisprudenciaArg.vue';\nimport DoctrinaArg from './components/DoctrinaArg.vue';\nimport ProximaCont from './components/ProximaCont.vue';\nimport ContactForm from './components/ContactForm.vue';\nimport FooterCont from './components/FooterCont.vue';\nexport default {\n  name: 'App',\n  components: {\n    NavBar,\n    LegislacionArg,\n    JurisprudenciaArg,\n    DoctrinaArg,\n    ProximaCont,\n    ContactForm,\n    FooterCont\n  },\n  data() {\n    return {\n      rutaActual: '' // Variable para almacenar la ruta actual\n    };\n  },\n\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n  },\n  unmounted() {\n    window.removeEventListener('scroll', this.handleScroll);\n  },\n  watch: {\n    $route(to) {\n      // Actualizar la ruta actual al cambiar de ruta\n      this.rutaActual = to.name;\n    }\n  },\n  methods: {\n    handleScroll() {\n      const scrollOffset = window.pageYOffset;\n      const heading = document.querySelector('.animated-heading');\n      if (heading) {\n        const distanceFromTop = heading.offsetTop;\n        const opacity = 1 - (scrollOffset - distanceFromTop) / distanceFromTop;\n        if (opacity >= 0 && opacity <= 1) {\n          heading.style.opacity = opacity;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavBar","LegislacionArg","JurisprudenciaArg","DoctrinaArg","ProximaCont","ContactForm","FooterCont","name","components","data","rutaActual","mounted","window","addEventListener","handleScroll","unmounted","removeEventListener","watch","$route","to","methods","scrollOffset","pageYOffset","heading","document","querySelector","distanceFromTop","offsetTop","opacity","style"],"sources":["C:\\Users\\combo 3 mini\\OneDrive\\jcac23017\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <main>\n      <header>\n        <NavBar></NavBar>\n        <transition name=\"fade\">\n          <h1 class=\"animated-heading\" v-show=\"rutaActual === '' || rutaActual === 'encabezado'\">\n            CONTENIDO-JURÍDICO.ARG\n          </h1>\n        </transition>\n      </header>\n      <transition name=\"fade\" mode=\"out-in\">\n        <router-view></router-view>\n      </transition>\n      <section class=\"column-container\" v-show=\"rutaActual === 'columnas'\"> \n        <LegislacionArg></LegislacionArg>\n        <JurisprudenciaArg></JurisprudenciaArg>\n        <DoctrinaArg></DoctrinaArg>\n        <ProximaCont></ProximaCont>\n      </section>\n      <section id=\"contacto\" v-show=\"rutaActual === 'contacto'\">\n        <div class=\"section-content\">\n          <h2>Contacto</h2>\n          <p>Aquí encontrarás contacto con nosotros.</p>\n          <ContactForm></ContactForm>\n        </div>\n      </section>\n      <footer>\n        <FooterCont></FooterCont>\n      </footer>\n    </main>\n  </div>\n</template>\n\n<script>\nimport NavBar from './components/NavBar.vue';\nimport LegislacionArg from './components/LegislacionArg.vue';\nimport JurisprudenciaArg from './components/JurisprudenciaArg.vue';\nimport DoctrinaArg from './components/DoctrinaArg.vue';\nimport ProximaCont from './components/ProximaCont.vue';\nimport ContactForm from './components/ContactForm.vue';\nimport FooterCont from './components/FooterCont.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    NavBar,\n    LegislacionArg,\n    JurisprudenciaArg,\n    DoctrinaArg,\n    ProximaCont,\n    ContactForm,\n    FooterCont,\n  },\n  data() {\n    return {\n      rutaActual: '', // Variable para almacenar la ruta actual\n    };\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n  },\n  unmounted() {\n    window.removeEventListener('scroll', this.handleScroll);\n  },\n  watch: {\n    $route(to) {\n      // Actualizar la ruta actual al cambiar de ruta\n      this.rutaActual = to.name;\n    },\n  },\n  methods: {\n    handleScroll() {\n      const scrollOffset = window.pageYOffset;\n      const heading = document.querySelector('.animated-heading');\n\n      if (heading) {\n        const distanceFromTop = heading.offsetTop;\n        const opacity = 1 - (scrollOffset - distanceFromTop) / distanceFromTop;\n\n        if (opacity >= 0 && opacity <= 1) {\n          heading.style.opacity = opacity;\n        }\n      }\n    },\n  },\n};\n</script>"],"mappings":"AAmCA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVR,MAAM;IACNC,cAAc;IACdC,iBAAiB;IACjBC,WAAW;IACXC,WAAW;IACXC,WAAW;IACXC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE,CAAE;IAClB,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACRC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,YAAY,CAAC;EACtD,CAAC;EACDC,SAASA,CAAA,EAAG;IACVH,MAAM,CAACI,mBAAmB,CAAC,QAAQ,EAAE,IAAI,CAACF,YAAY,CAAC;EACzD,CAAC;EACDG,KAAK,EAAE;IACLC,MAAMA,CAACC,EAAE,EAAE;MACT;MACA,IAAI,CAACT,UAAS,GAAIS,EAAE,CAACZ,IAAI;IAC3B;EACF,CAAC;EACDa,OAAO,EAAE;IACPN,YAAYA,CAAA,EAAG;MACb,MAAMO,YAAW,GAAIT,MAAM,CAACU,WAAW;MACvC,MAAMC,OAAM,GAAIC,QAAQ,CAACC,aAAa,CAAC,mBAAmB,CAAC;MAE3D,IAAIF,OAAO,EAAE;QACX,MAAMG,eAAc,GAAIH,OAAO,CAACI,SAAS;QACzC,MAAMC,OAAM,GAAI,IAAI,CAACP,YAAW,GAAIK,eAAe,IAAIA,eAAe;QAEtE,IAAIE,OAAM,IAAK,KAAKA,OAAM,IAAK,CAAC,EAAE;UAChCL,OAAO,CAACM,KAAK,CAACD,OAAM,GAAIA,OAAO;QACjC;MACF;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}
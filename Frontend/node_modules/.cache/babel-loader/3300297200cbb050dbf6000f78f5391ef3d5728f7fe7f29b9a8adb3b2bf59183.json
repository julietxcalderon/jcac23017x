> \n          <h1>EL CALDERO DE JULIETA</h1>\n          <div class=\"logo\">\n          <img src=\"@/assets/logo.png\" alt=\"Logo\">\n          </div>\n      </section>\n\n      <section id=\"legislacion\">\n        <LegislacionArg></LegislacionArg>\n        <div id=\"apileyes\"></div>\n      </section>\n\n      <section id=\"jurisprudencia\">\n        <JurisprudenciaArg></JurisprudenciaArg>\n      </section>\n\n      <section id=\"doctrina\">\n        <DoctrinaArg></DoctrinaArg>\n      </section>\n\n      <section id=\"proximamente\">\n        <ProximaCont></ProximaCont>\n      </section>\n\n      <section id=\"contacto\">\n        <h2>Contacto</h2>\n        <p>En esta sección encontrarás contacto con nosotrxs.</p>\n        <ContactForm></ContactForm>\n      </section>\n\n    </main>\n    \n    <footer>\n      <FooterCont></FooterCont>\n    </footer>\n\n    \n\n  </div>\n</template>\n\n<script>\nimport NavBar from './components/NavBar.vue';\nimport LegislacionArg from './components/LegislacionArg.vue';\nimport JurisprudenciaArg from './components/JurisprudenciaArg.vue';\nimport DoctrinaArg from './components/DoctrinaArg.vue';\nimport ProximaCont from './components/ProximaCont.vue';\nimport ContactForm from './components/ContactForm.vue';\nimport FooterCont from './components/FooterCont.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    NavBar,\n    LegislacionArg,\n    JurisprudenciaArg,\n    DoctrinaArg,\n    ProximaCont,\n    ContactForm,\n    FooterCont,\n},\n  data() {\n    return {\n      mensaje: 'Bienvenidx a Contenido Jurídico Argentino'\n    };\n  }\n};\n\nfetch('http://datos.jus.gob.ar/dataset/base-de-datos-legislativos-infoleg/resource/base-infoleg-normativa-nacional-muestreo.csv')\n  .then(response => response.text())\n  .then(data => {\n\n    const rows = data.split('\\n');\n    const laws = rows.map(row => row.split(','));\n    let html = '';\n    laws.forEach(law => {\n      const title = law[8];\n      const description = law[11];\n      html += `<div class=\"ley\">\n                  <h3>${title}</h3>\n                  <p>${description}</p>\n              </div>`;\n    });\n\n    const ApiLeyes = document.getElementById('api-leyes');\n    ApiLeyes.innerHTML = html;\n  })\n  .catch(error => {\n    console.log('Error al obtener las leyes:', error);\n  });\n\n\n</script>\n\n<style>\nbody {\n  background-color: #0b0d0e;\n  color: #fff;\n}\n\nh1 {\n  font-size: 60px;\n  font-family: system-ui, 'Segoe UI', 'Open Sans';\n  text-align: center;\n  color: rgb(201, 199, 169);\n}\n\nh2, p {\n  color: #9bbbbb;\n}\n\n.container {\n  margin: 0 auto;\n  max-width: auto;\n}\n\n.dark-mode {\n  background-color: #000;\n  color: #fff;\n}\n\n.dark-mode h2, .dark-mode p {\n  color: #fff;\n}\n\n.dark-mode .container {\n  border: 1px solid #fff;\n}\n\n.container.minimal {\n  padding: 10px;\n}\n\n.logo {\n    margin-right: auto;\n    list-style: none;\n    margin: 0;\n    padding: 0;\n    display: flex;\n    justify-content: center; \n  }\n  \n  .logo img {\n    height: 300px;\n    width: auto;\n  }\n\n</style>\n"],"mappings":"AAkDA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVR,MAAM;IACNC,cAAc;IACdC,iBAAiB;IACjBC,WAAW;IACXC,WAAW;IACXC,WAAW;IACXC;EACJ,CAAC;EACCG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH;AACF,CAAC;AAEDC,KAAK,CAAC,0HAA0H,EAC7HC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACH,IAAG,IAAK;EAEZ,MAAMM,IAAG,GAAIN,IAAI,CAACO,KAAK,CAAC,IAAI,CAAC;EAC7B,MAAMC,IAAG,GAAIF,IAAI,CAACG,GAAG,CAACC,GAAE,IAAKA,GAAG,CAACH,KAAK,CAAC,GAAG,CAAC,CAAC;EAC5C,IAAII,IAAG,GAAI,EAAE;EACbH,IAAI,CAACI,OAAO,CAACC,GAAE,IAAK;IAClB,MAAMC,KAAI,GAAID,GAAG,CAAC,CAAC,CAAC;IACpB,MAAME,WAAU,GAAIF,GAAG,CAAC,EAAE,CAAC;IAC3BF,IAAG,IAAM;wBACSG,KAAM;uBACPC,WAAY;qBACd;EACjB,CAAC,CAAC;EAEF,MAAMC,QAAO,GAAIC,QAAQ,CAACC,c
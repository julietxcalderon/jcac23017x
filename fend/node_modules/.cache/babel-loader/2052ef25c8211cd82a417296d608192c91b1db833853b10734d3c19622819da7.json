nimationFrame(animate);\n      }\n\n      animate();\n    },\n  },\n};\n</script>\n\n<style>\n#encabezado {\n  overflow: hidden;\n}\n\n.text-container {\n  white-space: nowrap;\n}\n\n.animated-heading {\n  position: relative;\n}\n</style>"],"mappings":"AAqCA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,UAAS,MAAO,6BAA6B;AACpD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVR,MAAM;IACNC,cAAc;IACdC,iBAAiB;IACjBC,WAAW;IACXC,WAAW;IACXC,WAAW;IACXC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACLC,MAAMA,CAACC,EAAE,EAAE;MACT,IAAI,CAACH,UAAS,GAAIG,EAAE,CAACN,IAAI;IAC3B;EACF,CAAC;EACHO,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACPD,iBAAiBA,CAAA,EAAG;MAClB,MAAME,OAAM,GAAIC,QAAQ,CAACC,aAAa,CAAC,mBAAmB,CAAC;MAC3D,MAAMC,SAAQ,GAAIF,QAAQ,CAACC,aAAa,CAAC,iBAAiB,CAAC;MAC3D,IAAIE,IAAG,GAAID,SAAS,CAACE,WAAW;MAEhC,SAASC,OAAOA,CAAA,EAAG;QACjBF,IAAG,IAAK,CAAC;QACTJ,OAAO,CAACO,KAAK,CAACC,SAAQ,GAAK,cAAaJ,IAAK,KAAI;QAEjD,IAAIA,IAAG,IAAK,CAACJ,OAAO,CAACK,WAAW,EAAE;UAChCD,IAAG,GAAID,SAAS,CAACE,WAAW;QAC9B;QAEAI,qBAAqB,CAACH,OAAO,CAAC;MAChC;MAEAA,OAAO,CAAC,CAAC;IACX;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}
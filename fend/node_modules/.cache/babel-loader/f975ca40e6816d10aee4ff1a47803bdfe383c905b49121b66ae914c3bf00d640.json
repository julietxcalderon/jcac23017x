{"ast":null,"code":"import NavBar from './components/NavBar.vue';\nimport LegislacionArg from './components/LegislacionArg.vue';\nimport JurisprudenciaArg from './components/JurisprudenciaArg.vue';\nimport DoctrinaArg from './components/DoctrinaArg.vue';\nimport ProximaCont from './components/ProximaCont.vue';\nimport ContactForm from './components/ContactForm.vue';\nimport FooterCont from './components/FooterCont.vue';\nexport default {\n  name: 'App',\n  components: {\n    NavBar,\n    LegislacionArg,\n    JurisprudenciaArg,\n    DoctrinaArg,\n    ProximaCont,\n    ContactForm,\n    FooterCont\n  },\n  mounted() {\n    this.animateEncabezado();\n  },\n  methods: {\n    animateEncabezado() {\n      const textElement = this.$refs.text;\n      const textWidth = textElement.offsetWidth;\n      const containerWidth = textElement.parentNode.offsetWidth;\n      const duration = 10000;\n      if (textWidth > containerWidth) {\n        let currentTime = Date.now();\n        const animate = () => {\n          const elapsedTime = Date.now() - currentTime;\n          const progress = elapsedTime % duration / duration;\n          const newPos = -textWidth - containerWidth + progress * (textWidth + containerWidth);\n          textElement.style.transform = `translateX(${newPos}px)`;\n          requestAnimationFrame(animate);\n        };\n        animate();\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavBar","LegislacionArg","JurisprudenciaArg","DoctrinaArg","ProximaCont","ContactForm","FooterCont","name","components","mounted","animateEncabezado","methods","textElement","$refs","text","textWidth","offsetWidth","containerWidth","parentNode","duration","currentTime","Date","now","animate","elapsedTime","progress","newPos","style","transform","requestAnimationFrame"],"sources":["C:\\Users\\combo 3 mini\\OneDrive\\jcac23017\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <main>\n      <header>\n        <NavBar></NavBar>\n        <router-view></route